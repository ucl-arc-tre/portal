    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cypress": "^14.3.0",
    "cypress-axe": "^1.6.0",
    "eslint": "^9.25.1",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.5.3",
    "typescript": "^5",
